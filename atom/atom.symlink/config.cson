"*":
  "atom-material-ui":
    depth: true
    rippleAccentColor: true
    showTabIcons: "Show on all tabs"
  core:
    audioBeep: false
    disabledPackages: [
      "about"
      "background-tips"
      "bookmarks"
      "deprecation-cop"
      "go-to-line"
      "metrics"
      "open-on-github"
      "styleguide"
      "welcome"
      "language-python"
      "language-gfm"
    ]
    ignoredNames: [
      ".git"
      ".tx"
      ".DS_Store"
      "*.pyc"
      ".gitignore"
      "__pycache__"
      "target"
    ]
    packagesWithKeymapsDisabled: []
    projectHome: "~/Development"
    themes: [
      "nucleus-dark-ui"
      "atom-dark-fusion-syntax"
    ]
  editor:
    fontFamily: "Monaco for Powerline"
    invisibles: {}
    preferredLineLength: 100
    softWrap: true
  "exception-reporting":
    userId: "4a37235a-e117-0b9b-9c87-2f3364facc68"
  "git-diff":
    showIconsInEditorGutter: true
  "go-debug":
    panelInitialVisible: false
  "go-plus":
    autocomplete: {}
    goPath: "~/.golang/global"
    guru: {}
    lint:
      args: [
        "--vendor"
        "--disable-all"
        "--enable=vet"
        "--enable=vetshadow"
        "--enable=golint"
        "--enable=ineffassign"
        "--enable=goconst"
        "--tests"
        "--json"
        "--exclude=should\\shave\\scomment"
      ]
    panel:
      displayMode: "closed"
    panelDisplayMode: "closed"
  linter: {}
  "linter-eslint":
    fixOnSave: true
  "linter-php": {}
  "linter-pylint":
    executablePath: "~/.virtualenvs/atom/bin/pylint"
    pythonPath: "~/.virtualenvs/atom"
    rcFile: "~/.pylintrc"
  "linter-ui-default":
    panelHeight: 60
  "markdown-preview": {}
  "merge-conflicts": {}
  "one-dark-ui": {}
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "spell-check": {}
  "sync-settings":
    _analyticsUserId: "3c1ae906-b17c-4623-8c38-79b04ad1318a"
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  "vim-mode": {}
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
".editorconfig.source":
  editor:
    preferredLineLength: 100
    tabLength: 4
